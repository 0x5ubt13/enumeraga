package main

import (
	"fmt"
	"github.com/0x5ubt13/enumeraga/internal/utils"
)

func main() {
	testCases := []string{
		"google.com",
		"http://example.com",
		"https://github.com",
		"8.8.8.8",
		"192.168.1.1",
	}

	fmt.Println("Testing hostname resolution:")
	fmt.Println("============================")

	for _, test := range testCases {
		ip, err := utils.ResolveHostToIP(test)
		if err != nil {
			fmt.Printf("[-] %s -> Error: %v\n", test, err)
		} else {
			fmt.Printf("[+] %s -> %s\n", test, ip)
		}
	}
}
